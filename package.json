{
  "name": "eofol6",
  "version": "0.0.1",
  "scripts": {
    "start": "node ./scripts/start.js",
    "build": "node ./scripts/build.js",
    "serve": "node ./scripts/serve.js",
    "clean": "node ./scripts/clean.js",
    "analyze": "npm start -- --analyze",
    "reinstall": "node ./scripts/reinstall.js",
    "reinstall-f": "node ./scripts/reinstall.js -f",
    "check": "npx eslint -c eslint.config.mjs .",
    "check-fix": "npx eslint -c eslint.config.mjs --fix .",
    "prettify": "npx prettier . --write",
    "lint": "npm run check-fix && npm run prettify",
    "deploy": "node ./scripts/deploy.js",
    "release": "npm run clean && npm run build && npm run deploy",
    "depcheck": "npx depcheck"
  },
  "author": "Jakub Eliáš <wurducius@gmail.com>",
  "license": "MIT",
  "description": "All inclusive web framework with zero configuration, batteries included!",
  "dependencies": {
    "http-server": "14.1.1",
    "rimraf": "6.0.1",
    "webpack": "5.99.9",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-dev-server": "5.2.2"
  },
  "devDependencies": {
    "@eslint/js": "9.28.0",
    "eslint": "9.28.0",
    "globals": "16.2.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.33.1"
  }
}
